using "controlflow";

foo[T](a: T) {}

f[T](x: T)
{
  var a = 0;
  while {a < 5}
  {
    a = a + 1;

    if (a < 2)
    {
      a = a + 1;
      continue
    }
    else
    {
      break
    }
  }

  for x
  {
    a =>
    foo(a)
  }

  return a
}
